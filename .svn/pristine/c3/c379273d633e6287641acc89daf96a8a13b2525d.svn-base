package com.autouploader;

import javafx.scene.image.ImageView;

import java.util.Date;

public class Patient {
    private String name;
    private Integer caseId;
    private Integer mrn;
    private String uploadStatus;
    private String caseStatus;
    private Date dateCreated;
    private String uploadedBy;
    private ImageView imageView;

    public Patient(String name, Integer caseId, Integer mrn, String uploadStatus, String caseStatus, Date dateCreated, String uploadedBy, ImageView imageView) {
        this.name = name;
        this.caseId = caseId;
        this.mrn = mrn;
        this.uploadStatus = uploadStatus;
        this.caseStatus = caseStatus;
        this.dateCreated = dateCreated;
        this.uploadedBy = uploadedBy;
        this.imageView = imageView;
    }

    public ImageView getImageView() {
        return imageView;
    }

    public void setImageView(ImageView imageView) {
        this.imageView = imageView;
    }

    public void setName(String name) {
        this.name = name;
    }

    public void setCaseId(Integer caseId) {
        this.caseId = caseId;
    }

    public void setMrn(Integer mrn) {
        this.mrn = mrn;
    }

    public void setUploadStatus(String uploadStatus) {
        this.uploadStatus = uploadStatus;
    }

    public void setCaseStatus(String caseStatus) {
        this.caseStatus = caseStatus;
    }

    public void setDateCreated(Date dateCreated) {
        this.dateCreated = dateCreated;
    }

    public void setUploadedBy(String uploadedBy) {
        this.uploadedBy = uploadedBy;
    }

    // Add getter methods for each property
    public String getName() {
        return name;
    }

    public Integer getCaseId() {
        return caseId;
    }

    public Integer getMrn() {
        return mrn;
    }

    public String getUploadStatus() {
        return uploadStatus;
    }

    public String getCaseStatus() {
        return caseStatus;
    }

    public Date getDateCreated() {
        return dateCreated;
    }

    public String getUploadedBy() {
        return uploadedBy;
    }

}
